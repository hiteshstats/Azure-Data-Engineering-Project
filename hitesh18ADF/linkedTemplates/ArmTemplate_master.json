{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "hitesh18ADF"
		},
		"ADLS_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_LS'"
		},
		"ADLS_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adls18.dfs.core.windows.net/"
		},
		"AmazonS3_LS_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "AzureKeyVault_LS",
					"type": "LinkedServiceReference"
				},
				"secretName": "S3AccessKey"
			}
		},
		"AzureKeyVault_LS_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvault1809.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "hitesh18ADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_LS_accountKey": {
						"value": "[parameters('ADLS_LS_accountKey')]"
					},
					"ADLS_LS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_LS_properties_typeProperties_url')]"
					},
					"AzureKeyVault_LS_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_LS_properties_typeProperties_baseUrl')]"
					},
					"AmazonS3_LS_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS3_LS_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}